+static int power_down(struct v4l2_subdev *sd)
+{
+	struct ov5693_device *dev = to_ov5693_sensor(sd);
+
+	dev->focus = OV5693_INVALID_CONFIG;
+
+	return gpio_crs_ctrl(dev, false);
+}
